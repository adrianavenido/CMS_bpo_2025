{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "cms/**/*.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/login.html"
    },
    {
      "src": "/login",
      "dest": "/login.html"
    },
    {
      "src": "/cms/auth/(.*)",
      "dest": "/cms/auth/$1"
    },
    {
      "src": "/cms/(.*)",
      "dest": "/cms/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 